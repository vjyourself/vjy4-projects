            {"n":"bindRouter","cn":"vjyourself4.ctrl.BindRouter",
              "setInit":false,
              "channels":{
                "ctrlGrammar":{
                  "ind":0,
                  "states":[
                    [
                      ["ctrlGrammar.trig","AnyRouter"],
                      ["ctrlGrammar.anal","AnyRouter"],
                      ],
                    [
                      ["ctrlGrammar.trig","globalVary"],
                      ["ctrlGrammar.anal","scene.anal"],
                    ]
                  ],
                }     
              }
            },
            
            {"n":"anyRouter","cn":"vjyourself4.ctrl.AnyRouter",
              "mapTrig":[
                {"t":"command","command":["globalVary","next",[2]]},
                {"t":"command","command":["globalVary","next",[0]]},
              ],
              "mapAnal":[
                {"t":"anal","tar":{"obj":"scene.anal","ch":4}},
                {"t":"anal","tar":{"obj":"scene.anal","ch":5}},
                {"t":"anal","tar":{"obj":"scene.anal","ch":6}},
                {"t":"anal","tar":{"obj":"scene.anal","ch":7}},
                 
